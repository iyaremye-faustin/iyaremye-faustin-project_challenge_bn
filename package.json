{
  "name": "company-name",
  "version": "1.0.0",
  "description": "crop-production mo api",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test  npm run sequelize && jest --forceExit",
    "test:dev": "env NODE_ENV=development && npm test",
    "test:watch": "jest --watch",
    "test:coverage": "cross-env NODE_ENV=test  npm run sequelize && jest --coverage",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register  ./dist/index.js",
    "pm2": "env NODE_ENV=production pm2 start ./dist/index.js -n 'agowebapi' --node-args='-r ts-node/register/transpile-only -r tsconfig-paths/register'",
    "build": "mkdir dist  && tsc -p tsconfig.prod.json",
    "db:prod": "env NODE_ENV=production npm db:create && npm db:migrate && npm db:seed npm build",
    "dev": "nodemon",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "commit": "cz",
    "sequelize": "npm run db:reset:all && npm run db:migrate && npm run db:seed",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:reset:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all --seeders-path ./src/database/jobs/seeders/seeders",
    "db:create:test": "env NODE_ENV=test npm run db:create",
    "db:drop:test": "env NODE_ENV=test npm run db:drop",
    "db:migrate:test": "env NODE_ENV=test npm run db:migrate",
    "db:seed:test": "env NODE_ENV=test npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Faustin",
  "license": "ISC",
  "homepage": "",
  "dependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/node": "^7.24.6",
    "@babel/plugin-transform-class-properties": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "env": "^0.0.2",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.8.1",
    "twilio": "^3.81.0",
    "typescript": "^4.7.4",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "body-parser": "^2.0.0-beta.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
